{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "no-undef": [
      1
    ],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "max-len": [
      1,
      90,
      2,
      {"ignoreComments": true
      }
    ],
    "one-var": 0,
    "one-var-declaration-per-line": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "function-paren-newline": [
      "error",
      "multiline"
    ],
    "curly": [
      "error",
      "multi-line"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "globals": {
      "localStorage": true,
      "document": true,
      "require": true,
      "window": true
    },
    "import/no-unresolved": [
      2,
      {
        "commonjs": true
      }
    ]
  }
}